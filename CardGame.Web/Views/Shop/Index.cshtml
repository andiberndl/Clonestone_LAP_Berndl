@model IEnumerable<CardGame.Web.Models.Shop>


@{
    ViewBag.Title = "Index";
}

<h2>Shop</h2>
@{
    for (int i = 0; i < 3; i++)
    {
        <div class="col-md-4">
            @Model.ToList()[i].Packname <br />
            @Model.ToList()[i].Packprice<br />
            @Model.ToList()[i].Cardquantity
        </div>
    }
}
